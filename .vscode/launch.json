{
  "version": "0.2.0",
  "configurations": [
    {
      "cwd": "${workspaceFolder}/docs",
      "command": "./node_modules/.bin/astro dev",
      "name": "Docs development server",
      "request": "launch",
      "type": "node-terminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Financial Research Agent",
      "runtimeExecutable": "npx",
      "runtimeArgs": [
        "--nolazy",
        "tsx",
        "examples/financial-research-agent/main.ts"
      ],
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    }
  ]
}
